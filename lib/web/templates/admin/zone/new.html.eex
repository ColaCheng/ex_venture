<section class="content-header">
  <h1>
    New Zone
  </h1>

  <ol class="breadcrumb">
    <li>
      <%= link(to: dashboard_path(@conn, :index)) do %>
        <i class="fa fa-dashboard"></i> Home
      <% end %>
    </li>
    <li><%= link("Zones", to: zone_path(@conn, :index)) %></li>
    <li class="active">New</li>
  </ol>
</section>

<section class="content">
  <div class="row">
    <div class="col-md-12">
      <%= render "_form.html", path: zone_path(@conn, :create), edit_name_layers: false, changeset: @changeset, title: "New", submit: "Create" %>
    </div>
  </div>
</section>
